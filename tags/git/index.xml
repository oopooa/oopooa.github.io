&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">[client]
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">default-character-set=utf8
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">[mysql]
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">default-character-set=utf8
